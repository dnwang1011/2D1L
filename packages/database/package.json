{
  "name": "@2dots1line/database",
  "version": "0.1.0",
  "description": "Database access layer for 2dots1line V7",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "clean": "rm -rf dist node_modules/.cache",
    "db:generate": "prisma generate --schema=./prisma/schema.prisma",
    "db:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./prisma/schema.prisma",
    "db:weaviate:schema": "tsx ./scripts/apply_weaviate_schema.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "denque": "^2.1.0",
    "form-data": "^4.0.2",
    "graphql-request": "^7.2.0",
    "ioredis": "^5.6.1",
    "neo4j-driver": "^5.28.1",
    "rxjs": "^7.8.2",
    "undici-types": "^7.10.0",
    "weaviate-ts-client": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prisma": "^6.8.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  }
}
